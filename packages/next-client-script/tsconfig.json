{
  "extends": "eslint-config-molindo/tsconfig.json",
  "include": ["src", "types"],
  "compilerOptions": {
    "module": "esnext",
    "lib": ["dom", "esnext"],
    "importHelpers": true,
    "declaration": true,
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "react",
    "esModuleInterop": true,
    "noUnusedParameters": true
  }
}
